{
	"name": "@supercolony-net/typechain-polkadot-dev",
	"version": "0.0.2",
	"scripts": {
		"start": "ts-node index.ts --in artifacts --out out",
		"deploy-to-test": "cd ./tests/deployed && yarn && yarn deploy:local",
		"gen-test-ts": "ts-node index.ts --in tests/deployed/artifacts --out tests/generated",
		"test": "jest --no-cache --maxWorkers 1",
		"lint": "eslint \"{src,tests}/**/*.{js,jsx,ts,tsx}\"",
		"lint:fix": "eslint --fix \"{src,tests}/**/*.{js,jsx,ts,tsx}\"",
		"link-dev": "npm link @supercolony-net/polkadot-ts-sdk",
		"_unlink-dev": "npm uninstall --no-save @supercolony-net/polkadot-ts-sdk && npm i",
		"unlink-dev": "npm unlink --no-save @supercolony-net/polkadot-ts-sdk && npm i"
	},
	"bin": "bin/index.js",
	"files": [
		"index.ts",
		"src",
		"bin",
		"tsconfig.json"
	],
	"engines": {
		"node": ">=16.0.0",
		"npm": ">=7.0.0"
	},
	"engineStrict": true,
	"dependencies": {
		"@types/fs-extra": "^9.0.13",
		"@types/node": "^17.0.34",
		"@types/yargs": "^17.0.10",
		"camelcase": "^6.3.0",
		"eslint": "^8.18.0",
		"fs-extra": "^10.1.0",
		"prettier": "^2.7.1",
		"ts-node": "^10.7.0",
		"yargs": "^17.5.1"
	},
	"devDependencies": {
		"@polkadot/api": "^8.6.2",
		"@polkadot/api-contract": "^8.6.2",
		"@polkadot/keyring": "^9.3.1",
		"@types/bn.js": "^5.1.0",
		"@types/eslint": "^8.4.3",
		"@types/jest": "^27.5.1",
		"@typescript-eslint/eslint-plugin": "^5.25.0",
		"@typescript-eslint/parser": "^5.25.0",
		"eslint": "^8.18.0",
		"eslint-import-resolver-typescript": "^2.7.1",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-node": "^11.1.0",
		"hjson": "^3.2.2",
		"ts-jest": "^28.0.3",
		"typescript": "^4.6.4"
	}
}